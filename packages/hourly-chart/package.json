    "@bufferapp/analyze-shared-components": "^0.12.0"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "3.1.4",
    "@storybook/react": "3.1.3",
    "eslint": "3.19.0",
    "jest": "19.0.2",
    "storybook-addon-a11y": "3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
