    "@bufferapp/analyze-shared-components": "^0.59.0",
    "@bufferapp/async-data-fetch": "0.5.36-beta01",
    "react-highcharts": "^12.0.0",
    "styled-components": "^2.2.1"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "3.1.4",
    "@storybook/react": "3.1.3",
