{
  "name": "@bufferapp/analyze-tabs",
  "version": "0.2.0",
  "description": "Tabs package for Insights",
  "main": "index.js",
  "scripts": {
    "start": "start-storybook -p 9001",
    "lint": "eslint . --ignore-pattern coverage node_modules",
    "test": "yarn run lint && sh ../../package_test.sh",
    "test-update": "jest -u"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@bufferapp/*)"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "author": "tik.hakobyan@gmail.com",
  "dependencies": {
    "@bufferapp/components": "2.1.1-beta.2"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "3.1.4",
    "@storybook/react": "3.1.3",
    "eslint": "3.19.0",
    "jest": "19.0.2",
    "storybook-addon-a11y": "3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
