  "scripts": {
    "bootstrap": "yarn",
    "clean": "lerna clean",
    "test": "yarn run lint && jest",
    "test-unit": "jest",
    "test-update": "jest -u snapshot.test.js",
    "snapshots": "jest snapshot.test.js",
    "init": "yarn install",
    "lint-branch": "git diff --name-only master... |  grep .jsx*$ | xargs eslint -c .eslintrc.json --stdin --quiet -f table",
    "lint-head": "git diff --name-only |  grep .jsx*$ | xargs eslint -c .eslintrc.json --stdin --quiet -f table",
    "lint": "yarn run lint-branch && yarn run lint-head",
    "start": "cd ./packages/server/ && yarn start",
    "storybook": "start-storybook -p 9001",
    "watch": "webpack-dev-server --config ./packages/server/webpack.config.dev.js --colors --display-error-details",
    "publish": "echo \"will eventually run: lerna publish\"",
    "build": "webpack  --config ./packages/server/webpack.config.prod.js --output-path=`pwd`",
    "postinstall": "npm rebuild grpc --target_arch=x64 --target_platform=linux --target_libc=glibc"
  },
  "private": true,
  "workspaces": [
    "packages/*"
  ],
